<Home>:
    name: 'Home'
    id: homescreen
    FloatLayout:
        id:list
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: 'Home_Screen_Blank.png'
        Label:
            text_size: self.size
            halign: 'center'
            valign: 'middle'
            pos:(0,385)
            text:'12:00PM'
            font_size: 24

        Label:
            text_size: self.size
            font_size: 110
            halign: 'center'
            valign: 'middle'
            pos:(0,290)
            text:'12:00PM'
            color:(0,0,0,1)

        Button:
            id:Music
            size_hint:(.23, .132)
            pos:(20, 375)
            opacity:0.1
            on_release: print("Music")
        Image:
            source:'resources/home_UI_icons/1 Music App.png'
            center:(Music.center_x,Music.center_y)

        Button:
            id:Contacts
            size_hint:(.23, .132)
            pos:(185, 375)
            opacity:0.1
            on_release: print("Contacts")
        Image:
            source:'resources/home_UI_icons/2 Contacts App.png'
            center:(Contacts.center_x,Contacts.center_y)

        Button:
            id:Mail
            size_hint:(.23, .132)
            pos:(350, 375)
            opacity:0.1
            on_release: print("Mail")
        Image:
            source:'resources/home_UI_icons/3 Mail App.png'
            center:(Mail.center_x,Mail.center_y)

        Button:
            id:Camera
            size_hint:(.23, .132)
            pos:(20, 235)
            opacity:0.1
            on_release: print("Camera")
        Image:
            source:'resources/home_UI_icons/4 Camera App.png'
            center:(Camera.center_x,Camera.center_y)

        Button:
            id:Settings
            size_hint:(.23, .132)
            pos:(185, 235)
            opacity:0.1
            on_release: print("Settings")
        Image:
            source:'resources/home_UI_icons/5 Settings App.png'
            center:(Settings.center_x,Settings.center_y)

        Button:
            id:Gallery
            size_hint:(.23, .132)
            pos:(350, 235)
            opacity:0.1
            on_release: print("Gallery")
        Image:
            source:'resources/home_UI_icons/6 Gallery App.png'
            center:(Gallery.center_x,Gallery.center_y)

        Button:
            id:Phone
            size_hint:(.23, .132)
            pos:(20, 95)
            opacity:0.1
            on_release: print("Phone")
        Image:
            source:'resources/home_UI_icons/7 Phone App.png'
            center:(Phone.center_x,Phone.center_y)

        Button:
            id:Browser
            size_hint:(.23, .132)
            pos:(185, 95)
            opacity:0.1
            on_release: print("Browser")
        Image:
            source:'resources/home_UI_icons/8 Broswer App.png'
            center:(Browser.center_x,Browser.center_y)

        Button:
            id:Texting
            size_hint:(.23, .132)
            pos:(350, 95)
            opacity:0.1
            on_release: print("Texting")
            on_press:
                root.manager.transition.direction = 'up'
                root.manager.transition.mode = 'push'
                root.switch()
        Image:
            source:'resources/home_UI_icons/9 Texting App.png'
            center:(Texting.center_x,Texting.center_y)
<Test>:
    name: 'Test'
    id: testscreen
    FloatLayout:
        id:convo
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: 'Blank_UI.png'
        Button:
            size: 100,100
            size_hint: None, None
            text: 'Not Real'
            on_press:
                root.manager.transition.direction = 'down'
                root.manager.transition.mode = 'pop'
                root.switch()
<ScreenManagement>:
    Home:
    Test: