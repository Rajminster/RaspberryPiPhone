#:kivy 1.8.0
<ListScreen>:
    name: 'list'
    id: listscreen
    FloatLayout:
        id:listlayout
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: '../Blank_UI.png'
        Button:
            size: 100,100
            pos: 200,100
            size_hint: None, None
            text: 'List'
            on_press:
                root.changer()
<OtherScreen>:
    name: 'other'
    id: otherscreen
    FloatLayout:
        id:otherlayout
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: '../Blank_UI.png'
        Button:
            size: 100,100
            pos: 200,100
            size_hint: None, None
            text: 'Other'
            on_press:
                root.changer()
<ScreenManagement>:
    ListScreen:
    OtherScreen:
